/* 弹窗 */

.fml-dialog-default {
    position: fixed;
    width: 540rpx;
    height: 500rpx;
    background: #f4b1c6;
    top: 50%;
    margin-top: -250rpx;
    left: 50%;
    margin-left: -270rpx;
    text-align: center;
    z-index: 2;
    box-sizing: border-box;
    border-radius: 20rpx;
}

.dialog-default-info {
    height: 350rpx;
    opacity: 1;
    border-radius: 20rpx;
    transform: translate3d(0, 0, 0);
    background: -webkit-gradient(linear, left top, left bottom, from(rgba(244, 244, 244, 0)), color-stop(0.1, #f4f4f4), to(#f4f4f4));
}

.dialog-default-head {
    height: 150rpx;
    color: #333;
    line-height: 150rpx;
    font-size: 80rpx;
    box-sizing: border-box;
    border-radius: 20rpx 20rpx 0 0;
}

.default-info-title {
    color: #000;
    font-size: 40rpx;
    padding-top: 70rpx;
}

.default-info-words {
    font-size: 30rpx;
    color: #6d6b6b;
    line-height: 1.5;
    padding: 30rpx;
    box-sizing: border-box;
    text-align: left;
}

.dialog-default-btn {
    color: #fff;
    font-size: 40rpx;
    height: 100rpx;
    line-height: 100rpx;
    background: #79d2d6;
    margin-top: 35rpx;
    border-radius: 0 0 20rpx 20rpx;
}


/* 提示语 */

.empty {
    position: fixed;
    width: 100%;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    z-index: 900;
    text-align: center;
    display: none;
}

.prompt {
    background: rgba(0, 0, 0, 0.7);
    width: 400rpx;
    padding: 20rpx;
    display: flex;
    align-items: flex-start;
    justify-content: center;
    text-align: left;
    border-radius: 10rpx;
    color: #fff;
}